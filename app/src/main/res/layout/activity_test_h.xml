<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:chx="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <com.jhy.org.yueqiu.view.ActionBarLayout
        chx:title_text="标题"
        chx:options_visible="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:onClick="testClick"
        android:background="@drawable/icon_launcher"
        android:layout_alignParentBottom="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageButton
        android:onClick="testClick"
        android:background="@drawable/icon_launcher"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="128dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.jhy.org.yueqiu.view.BaiduMapLayout
        android:id="@+id/baiduMap"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>
