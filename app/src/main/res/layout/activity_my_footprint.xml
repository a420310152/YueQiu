<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:chx="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.jhy.org.yueqiu.view.ActionBarLayout
        android:id="@+id/actionBar_footprint_title"
        chx:title_text="我的足迹"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/actionBar_footprint_title"
        android:divider="@color/dimgray"
        android:dividerHeight="2dp"
        android:layout_marginTop="10dp"
        android:id="@+id/lv_track">

    </ListView>
</RelativeLayout>
